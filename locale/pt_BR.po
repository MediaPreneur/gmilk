msgid "Gmilk configuration"
msgstr "Configuração do Gmilk"

#: configwindow.py:18
msgid "Interval (minutes)"
msgstr "Intervalo (minutos)"

#: configwindow.py:22
msgid "Ok"
msgstr "Ok"

#: configwindow.py:25
msgid "Cancel"
