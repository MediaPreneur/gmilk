msgid "Gmilk configuration"
msgstr "Configuração do Gmilk"

#: configwindow.py:19
msgid "Interval (minutes)"
msgstr "Intervalo (minutos)"

#: configwindow.py:23
msgid "Tags (comma separated)"
msgstr "Tags (separadas por vírgula)"

#: configwindow.py:27
msgid "Ok"
msgstr "Ok"

#: configwindow.py:30
msgid "Cancel"
msgstr "Cancela"
