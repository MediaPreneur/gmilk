#: gmilk.py:76
msgid "Asking the task list to Remember the Milk ..."
msgstr "Consultando a lista de tarefas no Remember the Milk ..."

#: gmilk.py:129
msgid "Authorize"
msgstr "Autorizar"

#: gmilk.py:132
msgid "Need to authorize. Click on 'Authorize' on the menu."
msgstr "Precisa de autorização. Clique em 'Autorizar' no menu."

#: gmilk.py:149
msgid "Configuration"
msgstr "Configuração"

#: gmilk.py:155
msgid "Check now!"
msgstr "Verificar agora!"

#: gmilk.py:162
msgid "About"
msgstr "Sobre"

#: gmilk.py:168
msgid "Quit"
msgstr "Sair"

#: gmilk.py:187
msgid "Creating a timeline ..."
msgstr "Criando uma linha de tempo ..."

#: gmilk.py:190
msgid "Checking your tasks ..."
msgstr "Verificando suas tarefas ..."

#: gmilk.py:206
msgid "No tasks today"
msgstr "Sem tarefas hoje"

#: gmilk.py:206
msgid "Today tasks"
msgstr "Tarefas de hoje"

#: gmilk.py:207
msgid "No tasks tomorrow"
msgstr "Sem tarefas amanhã"

#: gmilk.py:207
msgid "Tomorrow tasks"
msgstr "Tarefas de amanhã"

#: gmilk.py:208
msgid "No due tasks"
msgstr "Sem tarefas vencidas"

#: gmilk.py:208
msgid "Due tasks"
msgstr "Tarefas vencidas"

#: gmilk.py:218
msgid "Tasks alert"
msgstr "Alerta de tarefas"

#: gmilk.py:282
#, python-format
msgid "%s tasks found."
msgstr "%s tarefas encontradas."

#: gmilk.py:295
msgid "%m/%d/%Y"
msgstr "%d/%m/%Y"

#: gmilk.py:296
#, python-format
msgid "- %(title)s due on %(date)s"
msgstr "- %(title)s vencida em %(date)s"

#: gmilk.py:310
#, python-format
msgid "%d tasks found."
msgstr "%d tarefas encontradas."

#: gmilk.py:317
#, python-format
msgid "Are you sure you want to mark task '%s' as completed?"
msgstr "Tem certeza que deseja marcar a tarefa '%s' como completa?"

#: gmilk.py:323
#, python-format
msgid "Marking '%s' task as complete ..."
msgstr "Marcando tarefa '%s' como completa ..."

#: gmilk.py:328
#, python-format
msgid "Task '%s' marked as completed."
msgstr "Tarefa '%s' marcada como completa."

#: gmilk.py:340
msgid "Could not mark task as complete."
msgstr "Não consegui marcar a tarefa como completa."

#: gmilk.py:343
msgid "There was an error marking task as complete."
msgstr "Houve um erro marcando a tarefa como completa."

#: gmilk.py:384
msgid "Please enter the following URL on your browser"
msgstr "Por favor informe o seguinte endereço no seu navegador"

#: gmilk.py:386
msgid "Asking authorization"
msgstr "Requisitando autorização"

#: gmilk.py:399
msgid "You need authorization to use this app"
msgstr "Você precisa de autorização para usar esse programa"

#: gmilk.py:405
msgid "Did you give authorization to this app? Please try again."
msgstr ""
"Você requisitou autorização para esse programa? Por favor tente novamente."

#: gmilk.py:411
msgid "Authorized! You should now be able to use this app now."
msgstr "Autorizado! Você deverá poder usar esse programa agora."

#: gmilk.py:415
msgid "Invalid authorization, please try again."
msgstr "Autorização inválida, por favor tente novamente."

